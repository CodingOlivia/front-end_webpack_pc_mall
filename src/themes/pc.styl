@font-face
  font-family: 'HelveticaNeue'
  src url('~statics/font/HelveticaNeue/HelveticaNeue.eot')
  src url('~statics/font/HelveticaNeue/HelveticaNeue.eot?#iefixed') format('embedded-opentype'),
    url('~statics/font/HelveticaNeue/HelveticaNeue.woff2') format('woff2'),
    url('~statics/font/HelveticaNeue/HelveticaNeue.woff') format('woff'),
    url('~statics/font/HelveticaNeue/HelveticaNeue.ttf') format('truetype'),
    url('~statics/font/HelveticaNeue/HelveticaNeue.svg') format('svg')

@font-face
  font-family: 'HelveticaNeue-75bold'
  // src: url('~statics/font/HelveticaNeue-75bold.ttf')
  src url('~statics/font/HelveticaNeue/HelveticaNeue.eot')
  src url('~statics/font/HelveticaNeue/HelveticaNeue.eot?#iefixed') format('embedded-opentype'),
    url('~statics/font/HelveticaNeue/HelveticaNeue.woff2') format('woff2'),
    url('~statics/font/HelveticaNeue/HelveticaNeue.woff') format('woff'),
    url('~statics/font/HelveticaNeue/HelveticaNeue.ttf') format('truetype'),
    url('~statics/font/HelveticaNeue/HelveticaNeue.svg') format('svg')
  font-weight bold

$pc_normal_font = 'HelveticaNeue'
@import '~themes/app.variables'

html[app='desktop'], html[app='desktop'] body
  font-family $pc_normal_font
html[app='desktop'] body
  *
    user-select none
    letter-spacing 0px
  input, textarea
    user-select auto
  p 
    letter-spacing 1px
  span 
    letter-spacing 1px
  .transition
    transition all .3s ease-in

  .transition-linear
    transition all .3s linear
  // home start
  .slide-trans-enter-active
     transition opacity .5s
  .slide-trans-enter
    transform translateX(1920px)
  .slide-trans-leave-active
    transition opacity .5s
    transform translateX(-1920px)
  .slide-trans-leave-to
    opacity 0
  .box
    display flex
    flex-direction column
    justify-content center
    .item1
      width 100%
      position relative
      .home-banner
        width 100%
        height 100vh
        display flex
        overflow hidden
        position relative
        >.homeBanner
          width 100%
          height 100%
          display flex
          overflow hidden
          position relative
      .banner-image
        width 100%
        display flex
        flex-shrink 0
        justify-content center
        align-items center
        position relative
        > span
          width 100%
          height 100%
          position relative
      .imagesbox .q-carousel-toolbar
        display none
      .imagesbox .q-icon .material-icons
        width 80px
        height 80px
      .q-carousel-left-button i, .q-carousel-right-button i
        padding 8px
      .banner-icon
        width 108px
        position absolute
        z-index 100
        display flex
        justify-content center
        align-items center
        flex-direction column
        align-self flex-end
        right calc(64 / 1920 * 100vw)
        bottom calc(80 / 1920 * 100vw)
        .icon-img
          display flex
          flex-direction row
          justify-content center
          align-items center
          padding 8px 0
          >i
            width 40px
            height 40px
            cursor pointer
          >.icon-left
            background url('~statics/images/Homepage/L.png') top center / 100% no-repeat
            margin-right 8px
            &:hover
              background url('~statics/images/Homepage/L1.png') top center / 100% no-repeat
          >.icon-right
            background url('~statics/images/Homepage/R.png') top center / 100% no-repeat
            margin-left 8px
            &:hover
              background url('~statics/images/Homepage/R1.png') top center / 100% no-repeat
        .icon-left
          padding 0 6%
        .icon-right
          padding 0 6%
        .text
          width 100%
          color $all-color-1
          display flex
          justify-content space-between
          align-items center
          font-family HelveticaNeue
          font-weight bold
          font-size 14px
          >span
            display flex
            letter-spacing 2px
          hr
            width 64px
            color white
            height calc(2 / 1920 * 100vw)
            border-width 0px
            background $all-color-2
          .text1
            padding 0 6%
          .text2
            color $all-color-2
            padding 0 6%
      .banner-title
        font-family HelveticaNeue
        position absolute
        z-index 100
        align-self flex-end
        left calc(64 / 1920 * 100vw)
        bottom calc(80 / 1920 * 100vw)
      .banner-name
        font-size calc(64/1920*100vw)
        font-weight bold
        color $all-color-1
        margin-bottom 11%
      .banner-price
        font-size calc(32/1920*100vw)
        color $all-color-1
      .banner-category
        font-size calc(32/1920*100vw)
        color $all-color-2
        margin-left calc(24/1920*100vw)
      .banner-btn
        font-weight bold
        font-size calc(14/1920*100vw)
        color $all-color-1
        background white
        width calc(110/1920*100vw)
        height calc(52/1920*100vw)
        border-radius 50px
        opacity 0.80
        box-shadow 0px 0px 0px $all-color-2
        margin 3.2% 0 0 0
    .item2
      display flex
      flex-direction column
      justify-content center
      align-items flex-start 
      width 100%
      .products-title
        display flex
        justify-content center
        width 100%
        font-weight bold
        font-size calc(24 / 1920 * 100vw)
        color $all-color-1
        .products-title1
          font-family HelveticaNeue
          width 81.6%
          font-weight bold
          font-size calc(24 / 1920 * 100vw)
          color $all-color-1
          margin calc(120 / 1920 * 100vw) 0 calc(56 / 1920 * 100vw) 0
      .products-img
        display flex
        flex-direction row
        justify-content center
        align-items flex-start
        width 100%
      .new-products
        font-family HelveticaNeue
        width 18%
        display flex
        flex-direction column
        justify-content center
        align-items flex-start 
        border 2px solid transparent
        margin calc(16 / 1920 * 100vw) calc(32 / 1920 * 100vw) calc(16 / 1920 * 100vw) calc(32 / 1920 * 100vw)
        &:hover
          border 2px solid $all-color-1
        &.active
          border 2px solid $all-color-1
      .products-name
        width 100%
        color $all-color-1
        font-weight bold
        font-size calc(18 / 1920 * 100vw)
        text-align center
        padding 0px 16px 0px 16px
        line-height 1
        margin 0 0 calc(6 / 1920 * 100vw) 0
      .price-type
        width 100%
        text-align center
        padding 0px 8px 0px 8px
        font-size calc(16 / 1920 * 100vw)
        margin 0 0 calc(64 / 1920 * 100vw) 0
      .products-price
        color $all-color-1
        font-weight bold
        letter-spacing 1px
      .products-type
        color $font-color-3
    .item3
      display flex
      flex-direction column
      justify-content center
      align-items flex-start 
      width 100%
      .featured-title
        display flex
        height calc(134 / 1920 * 100vw)
        justify-content: center;
        width 100%
        font-weight bold
        font-size calc(24/1920*100vw)
        color $all-color-1
        display flex
        align-items center
      .featured-title1
          width 81.6%
          height 100%
          font-family HelveticaNeue !important
          font-weight bolder
          font-size calc(24/1920*100vw)
          color $all-color-1
          display flex
          align-items center
      .featured
        display flex
        flex-direction row
        justify-content center
        align-items flex-start
        width 100%
        .new-featured
          width 81.4%
          position relative
        .featured-img
          width 88%
          margin 0 0 0 15%
          display flex
          align-items flex-end
      .featuredBox
        display flex
        align-items flex-end
        margin-bottom 46px
        position relative
      .featuredBox:nth-child(even) .featured-img
        width 88%
        margin 0 12% 0 0
      .featuredBox:nth-child(even) .featured-font
        width calc(464/1920*100vw)
        height calc(370/1920*100vw)
        background $all-color-2
        border 1px solid #f1f1f1
        position absolute
        display flex
        justify-content center
        align-items center
        right 0
      .featured-font
        width calc(464/1920*100vw)
        height calc(370/1920*100vw)
        background $all-color-2
        border 1px solid #f1f1f1
        position absolute
        left 0
        bottom 0
        display flex
        justify-content center
        align-items center
      .featured-font.right
        left auto
        right 0
        bottom 0
      .featured-body
        font-family HelveticaNeue !important
        width calc(324/1920*100vw)
      .featured-name
        font-size 24px
        font-weight bold
        color $all-color-1
        line-height 1
        margin 4% 0 0 0
      .featuredprice-type
        line-height 1
        margin 4% 0 8% 0
      .featured-price
        font-size 16px
        color $all-color-1
        line-height 1
        margin 1% 0 25px 0
        font-weight bold
        letter-spacing 0
      .featured-type
        font-size 16px
        color $font-color-3
        line-height 0
        letter-spacing 0
      .featured-describe
        width calc(324/1920*100vw)
        font-size 14px
        letter-spacing 0px
        color $all-color-3
        margin-bottom calc(48 / 1920 * 100vw)
        line-height 1.6
        display -webkit-box
        /* autoprefixer: off */  
        -webkit-box-orient: vertical;  
        /* autoprefixer: on */  
        -webkit-line-clamp 3
        overflow hidden
      .featured-btn1
        width calc(112/1920*100vw)
        height calc(36/1920*100vw)
        background $all-color-2
        border 1px solid $border-color-1 
        border-radius 50px
        font-size calc(14/1920*100vw)
        color $all-color-1
        box-shadow 0 0 0 white
        min-width 72px
        min-height 24px
        font-family HelveticaNeue !important
        font-weight bold
      .featured-btn2
        width calc(80/1920*100vw)
        height calc(36/1920*100vw)
        background $all-color-1
        border-radius 50px
        font-size calc(14/1920*100vw)
        color $all-color-2
        box-shadow 0 0 0 white
        margin-left calc(24/1920*100vw)
        font-weight normal
        min-width 54px
        min-height 24px
        font-family HelveticaNeue !important
        font-weight bold
    .see-all
      display flex
      justify-content center
      align-items flex-start
      width 100%
      .see-all-btn1
        background $all-color-1
        font-size calc(16/1920*100vw)
        color $all-color-2
        width calc(302/1920*100vw)
        height calc(64/1920*100vw)
        border-radius 50px
        font-weight normal
        margin 3.2% 0 0 0
        min-width 180px
        box-shadow 0 0 0 white
    .bottom-hr
      width 100%
      height 1px
      margin 3.2% 0
      display flex
      justify-content center
      .bottom-hr2
        width 81.2%
        background $border-color-1 
    .item4
      width 100%
      display flex
      justify-content center
      align-items flex-start
      font-family HelveticaNeue !important
      .sign-up
        width 81.2%
      .sign-title
        width 100%
        color $all-color-1
        font-weight bold
        font-size calc(24/1920*100vw)
        text-align center
        line-height 1
      .sign-title2
        width 100%
        color $all-color-3
        font-size calc(14/1920*100vw)
        text-align center
        line-height 1
      .signup-input
        display flex
        justify-content center
        align-items flex-start
      .signup-input2
        display flex
        justify-content space-between
        align-items center
        width calc(548/1920*100vw)
        height calc(64/1920*100vw)
        background $all-color-2 !important
        border-radius 50px
        border 1px solid $all-color-3
        margin 1% 0 1% 0
      .sign-input
        width calc(360/1920*100vw)
        height calc(50/1920*100vw)
        background rgba(0,0,0,0) !important
        color $all-color-3 !important
        font-size calc(14/1920*100vw)
        box-shadow 0 0 0 white
        margin 0 0 0 6%
        border 0
        outline none
      .signup-btn1
        width calc(136/1920*100vw)
        height calc(64/1920*100vw)
        background rgba(0,0,0,0) !important
        color $all-color-1 !important
        font-size calc(14/1920*100vw)
        box-shadow 0 0 0 white
        border 1px solid $all-color-3
        border-width 0 0 0 1px
        min-width 80px
        border-radius 0 50px 50px 0
        padding 0
    .home-footer
      margin 0
      background $all-color-2
      display flex
      justify-content center
      margin-top calc(76/1920*100vw)
    .layout-footer
      box-shadow 0 0 0 white
    .q-if-inverted .q-if-control
      color #000
    .q-if-dark .q-input-target
      color $all-color-3
    .q-btn-standard
      min-height 24px
  // home end
  // search start
  .search-box
    width 100%
    position absolute
    .q-ripple-container
      display none !important
    .navigation
      width 100%
      display flex
      justify-content center
      align-items center
      flex-direction column
      .move
        width 100%
        display flex
        justify-content flex-start
        align-items center
        padding 0 32px
        .products-layout-top
          height 64px
          // margin-left 32px
          .search-filter-Reset
            height 63px
        .container-top
          height 64px
          // margin-right 32px
          margin-left 16px
          // padding-right 16px
          width 83.4%
          .material-icons
            color rgba(0, 0, 0, 0)
            background url('~statics/search/images/new/down.png') top center no-repeat
            height 7px
          .q-if-focused
            .material-icons
              color rgba(0, 0, 0, 0)
              background url('~statics/search/images/new/down.png') top center no-repeat
              height 7px
              transform scale(1,-1)
          .second-filter-title-right
            height 63px
            .selectBox
              display flex
              margin 0
              color $all-color-1
              font-weight 600
              .selectInner
                font-size 14px
                display flex
                justify-content space-between
                align-items center
                >span:first-child
                  margin-right 8px
                  color #666
                >span:last-child
                  display flex
                  align-items center
                  cursor pointer
                .select-icon
                  height 7px
                  display flex
                  align-items center
                  padding-left 16px
                  background url('~statics/search/images/new/down.png') center no-repeat
                  margin-left 14px
                  &.down
                      transform:rotate(0deg)
                  &.up
                    transform:rotate(180deg)
              ul
                margin 30px 0 0 0
                background $all-color-2
                border 1px solid $border-color-1 
                text-align left
                min-width 146px
                position absolute
                right 48px
                li
                  list-style none
                  line-height 3
                  font-size 14px
                  margin-left 20px
                  border-bottom 1px solid $border-color-1 
                  margin-right 20px
                  cursor pointer
                  &:hover
                    color $all-color-3
                .selectLi
                  color $all-color-3
      .search-filter-Reset-hr
        border-bottom 1px solid #e5e5e5
        width 96%
        height 1px       
    .layout-header
      box-shadow none
      transform: translateY(0px)
      position relative !important
      z-index 200
    .layout-page-container.transition-generic
      padding-top 0 !important
      position relative
      z-index 100
    .q-item
       font-size calc(14/1920*100vw)
    .layout-page
     overflow visible
    .layout-aside.fixed.on-layout
      box-shadow none
    .heardTopbg
      position relative
      display flex
      height calc(318/1920*100vw)
      flex-direction column
      justify-content center
      align-items center
      .heardToptext
        line-height 1.6
        .heardToptext-type
         color $font-color-5
         font-size calc(24/1920*100vw)
        .heardToptext-name
         color $all-color-1
         font-size calc(34/1920*100vw)
         font-weight bold
    .heardTop-search-body
      width 100%
      display flex
      flex-direction column
      justify-content center
      align-items center
      position relative
      .q-if:before
        height 0px
      .q-if
        >i
          // display none
          width 28px
          height 28px
        >i.q-if-control-before
          display flex
          background url('~statics/images/Homepage/search.png') top center no-repeat
          color rgba(255,255,255,0)
          width 20px
          height 20px
          font-size 17px
          margin-right 8px
      .q-icon
        font-size calc(28/1920*100vw)
        color #b8b8b8
        margin-right 10px
      .search-autocomplete
        position absolute
        top 50%
        z-index 200
        width calc(338/1920*100vw)
        height auto
        max-height 240px
        box-sizing border-box
        border 1px solid #e5e5e5
        border-top none
        margin-top calc(36/1920*100vw)
        background #fff
        overflow-y auto
        >div
          height 48px
          margin 0 calc(32/1920*100vw)
          box-sizing border-box
          border-bottom 1px solid #e5e5e5
          display flex
          align-items center
          cursor pointer
          &:hover>span, &:hover>.active
            color #6abcfc
          >span
            font-size 14px
            color #b2b2b2
            &.active
              color #252525
    .heardTop-search
      width calc(338/1920*100vw)
      height calc(56/1920*100vw)
      box-sizing border-box
      border 1px solid $border-color-1 
      color $all-color-1
      background $all-color-2
      min-width 240px
      min-height 38px
      &.q-if-focused
        border 2px solid #252525
    .q-if
      display flex
      padding 0
      padding-left 10px
      box-shadow 0 0 0 white
      // flex-direction row-reverse
    .text-primary
      color white !important
    .layout-aside-left
      width 262px
      z-index 220
    .products-layout
      margin 0 28px
      padding 0 0 0 28px
      .q-icon
        width 10px
        height 10px
        font-size 12px
        color $all-color-3
        &.ion-close
          background url('~statics/images/Homepage/close.png') top center no-repeat
        &.ion-plus
          background url('~statics/images/Homepage/open.png') top center no-repeat
        &::before
          display none
      .products-list-title
        display flex
        justify-content space-between
        align-items center
      .products-title
        margin 0
        font-size 14px
        font-weight bold
        color $all-color-1
        padding 16px 0
      .products-open-img
        padding 0 32px 0 0
    .filter-type-more
      color $font-color-4
      font-size 14px
      padding 8px 0
      cursor pointer
    .filter-type1
      display flex
      ul
        padding 0
        font-size 14px
        li
          opacity: 0.6
          padding 8px 0
          color $all-color-1
          cursor pointer
          list-style-type none
          background $all-color-2
          padding 6px 0 6px 16px
          margin-left -16px
          margin-bottom 8px
          &:hover
            opacity 1
            color $background-color-4
            background $background-color-3
          &.active
            opacity 1
            color $background-color-4
            background $background-color-3
    .filter-type
      display flex
      ul
        padding 0
        font-size 14px
        .active
          opacity 1
          color $background-color-4
          background $background-color-3
        li
          opacity 0.7
          padding 8px 0
          color $all-color-1
          cursor pointer
          list-style-type none
          padding 6px 0 6px 16px
          margin-left -16px
          margin-bottom 8px
    .second-filter-title
      width calc(100% - 72px)
      min-height auto
      margin 20px 36px 0
      display flex
      justify-content flex-start
      align-items center
    .second-filter-title-right
      margin 0
      display flex
      justify-content space-between
      align-items center
      .second-filter-title-number
        font-size 14px
        color $font-color-5
        display flex
        font-weight bold
      .second-filter-title-select
        font-size 14px
        color $font-color-5
        display flex
        align-items center
        // padding-right 16px
        >span
          font-weight bold
        .q-if
          display flex
          flex-direction row
          padding 0 0 0 4px
          margin 0
        .q-input-target
          font-size 14px
          font-weight bold
        .q-if-control
          font-size calc(24/1920*100vw)
        .q-if:before
          height 0px
        .q-if:after
          height 0px
    .q-list
      padding 0
    .q-item-main
      font-size calc(14/1920*100vw)  
    .second-filter-title-left
      width 100%
    .search-filter-Reset
      width 262px
      display flex
      justify-content flex-start
      align-items center
      .filter-filter
        font-size 14px
        width 80px
        border-right 1px solid $border-color-1 
        margin 0 0 0 28px
        cursor pointer
        font-weight bold
      .filter-clear
        width 80px
        color $font-color-4
        margin-left 16px
        font-size 14px
        cursor pointer
        font-weight bold
        letter-spacing 1px
        span
          text-transform none
          justify-content flex-start
          font-size 14px
      .q-btn
        box-shadow 0 0 0 white
        .q-hoverable
          background transparent
    .search-filter-Reset-hr
      width 96%
      height 1px
      // background $border-color-1 
      margin-bottom 1.4%
      margin-top -10px
    .search-left-hr
        background $border-color-1 
        width 110%
        height 1px
        margin 16px 0 16px -16%
    .select-color-body
        width 170px
    .filter-color
      .products-title
        padding 16px 0
      .select-color-title
        padding 24px 0
        color $font-color-5
      .select-color-body
        .btn-border:hover
          span
            display flex
            color $font-color-5
            font-size 12px
            margin-top 16px
            text-transform capitalize
        .btn-border.active
          span
            display flex
            color $font-color-5
            font-size 12px
            margin-top 16px
            text-transform capitalize
            letter-spacing 0px
        .btn-border
          width 32px
          height 32px
          border-radius 50px
          border 1px solid $all-color-2
          display flex
          justify-content center
          align-items center
          margin 18px 6px
          span
            display none
          &:hover
            border 1px solid #e7e7e7
          &.active
            border 1px solid #7bdab1
          &:nth-child(1)
            margin-left 0
          &:nth-child(5)
            margin-left 0
          &:nth-child(6)
            .q-btn-standard
              background url('~statics/search/images/new/multi.png') !important
          &:nth-child(9)
            margin-left 0
          &:nth-child(13)
            margin-left 0
          &:nth-child(17)
            margin-left 0
          &:nth-child(21)
            margin-left 0
          &:nth-child(25)
            margin-left 0
        .q-btn-round.q-btn-standard
          width 24px
          height 24px
          min-height 24px
          font-size 24px !important
          box-shadow 0px 1px 2px #8d8d8d inset
    .filter-memory
        ul
          padding 0
          font-size calc(14/1920*100vw)
        .active
          opacity 1
          color $background-color-4
          background $background-color-3
        li
          opacity 0.7
          padding 8px 0
          color $all-color-1
          cursor pointer
          list-style-type none
          padding 6px 0 6px 16px
          margin-left -16px
          margin-bottom 8px
          &:hover
            opacity 1
            color $background-color-4
            background $background-color-3
      .select-memory-title
        padding 12px 0
        color $font-color-5
      .q-btn-standard
        margin 8px 0
