/* 输入框 */
.IWin-input-wrap

    position relative
    vertical-align middle
  
    .IWin-input[type="text"]
        padding-left 10px
        width 100%
        padding 20px 16px
        border 1px solid $border-color-1
        border-radius 2px
        box-sizing border-box
        font-size 14px
        outline none
        height 60px
        font-weight bold
        line-height 18px
        color $all-color-1 
        &::-webkit-input-placeholder
            color $all-color-3 
            font-weight normal
        &:-ms-input-placeholder
            color $all-color-3 
            font-weight normal
    .is-danger
        border-color #e50017!important
    .help
        position absolute
        display block
        font-size 11px
        margin-top 2px
        color #e50017
        line-height 1.2em
    .q-if-inverted
        margin-top 0
        background-color #fff!important
        border 1px solid $border-color-1
        box-shadow none
        min-height 30px
        padding 0
        .q-input-target
            font-family font-r
            font-size 1rem
            color $font-color-5!important
    .q-if-error
        border 1px solid #e50017
    .carrier-icon
        width 20px
        height 20px
        display block
        background url("~statics/eye-close.png") no-repeat center center
        position absolute
        right 16px
        top 50%
        transform translateY(-50%)
        cursor pointer
        &.active
            background url("~statics/eye-open.png") no-repeat center center
.IWin-input-error
  .wid-input-notice
    display block
    position absolute
    padding 0 10px
    top -30px
    height 20px
    line-height 18px
    font-size 12px
    color #e50017
    font-family font-r
    background-color #f5f5f5
    border 1px solid #ccc
    border-radius 4px
    &:before
      content ""
      position absolute
      bottom -8px
      left 10px
      border-left 5px solid transparent
      border-right 5px solid transparent
      border-top 8px solid #ccc
      z-index 100
    &:after
      content ""
      position absolute
      bottom -7px
      left 11px
      border-left 4px solid transparent
      border-right 4px solid transparent
      border-top 7px solid #f5f5f5
      z-index 110
  .wid-input[type="text"]
    color #e50017
/* 下拉选择 */
.IWin-select
  //width 100%
  display block
  position relative
  vertical-align middle
  box-sizing border-box
  cursor pointer
  span
    width calc(100% - 6px)
    height 27px
    line-height 27px
    padding-left 10px
    border 0
    cursor pointer
  input[type="text"]
    width 100%
    height 60px
    padding 20px 16px
    border 1px solid $border-color-1
    border-radius 2px
    box-sizing border-box
    cursor pointer
    outline none
    color $all-color-1 
    font-weight bold
    line-height 18px
    &:-webkit-input-placeholder
       color $all-color-3 
       font-weight normal
    &:-ms-input-placeholder
       color $all-color-3 
       font-weight normal
  .is-danger
    border-color #e50017!important
  .help
    position absolute
    display block
    padding-left 0
    font-size 11px
    height 14px
    line-height 14px
    margin-top 2px
    color #e50017
  i
    display inline-block
    position absolute
    right 15px
    top 28px
    width 10px
    height 7px
    background url('~statics/search/images/new/down.png') center no-repeat
  .IWin-select-item
    position absolute !important
    width 100%!important
    background-color #fff
    border 1px solid $border-color-1
    border-top 0!important
    top 100%!important
    line-height 18px
    z-index 300
    max-height 120px
    margin 0!important
    min-width auto!important
    overflow auto
    padding 0 20px
    li
      font-size 14px
      color $font-color-5 
      list-style-type none
      line-height 3
      margin 0!important
      border-bottom 1px solid $border-color-1
      &:hover
        font-weight 600
  .not-allowed
    cursor not-allowed
  .allowed
    cursor pointer!important
//弹框样式
.model-inner 
    .model-title
        position absolute
        font-size 20px
        font-weight bold
        padding 24px
        top 0
        left 0
        width 100%
        background $all-color-2
        .model-close
            position absolute 
            top 24px
            right 24px
            cursor pointer
            width 18px
            height 18px
            display block
            background url('~statics/sign/close.png') top center no-repeat
    
    .model-footer
        position absolute
        padding 24px
        bottom 0
        left 0
        width 100%
        background $all-color-2
        color $font-color-5
        font-size 13px
        .m-right--small
            margin-right 16px
        .model-footer-inner
            position relative
            &.align-right
               text-align right
            .model-footer-action
                position absolute
                right 0
                top -8px
                &.static
                    position static
        .btn
            width calc(180/1920*100vw)
            height calc(32/1920*100vw)
            min-width 180px
            height 36px
            border-radius: 50px
            background-color $background-color-4
            border 1px solid $background-color-4
            color $all-color-2
            font-size 18px
            text-align center
            text-transform uppercase
            display inline-block
            cursor pointer
            line-height 32px
            &.negative
                background-color $all-color-2
                border 1px solid $border-color-1
                color $all-color-1 
            &.disable
                background-color $border-color-1
                color $all-color-2
