.l-e-popup-wrap
    position fixed
    width 458px
    top 50%
    left 50%
    transform translate(-50%, -50%)
    .popup-head
        position relative
        padding-left 1.25rem
        height 2.857rem
        line-height 2.857rem
        background-color #038cd6
        border-top-left-radius 4px
        border-top-right-radius 4px
        cursor move
        .popup-title
            display inline-block
            color #fff
            font-family font-m
            font-size 1.125rem
        .popup-close
            display inline-block
            position absolute
            width 1rem
            height 1rem
            right 1.25rem
            top 0.75rem
            background url('~statics/images/common/close.png') center no-repeat
    .p-popup-main
        background-color #fff
        padding-top 0
        border 1px solid #ccc
        border-top 0
        .p-panel-content
            min-height 0
        .p-name-item
            padding-bottom 1.429rem
            padding-top 1.429rem
        .p-catalog-item
            display flex
            border-bottom 1px solid #e5e5e5
        .p-link-edit-title
            font-family font-r
            font-size 16px
            color #333333
        .p-link-edit-subtitle
            font-family Roboto-Regular
            font-size 14px
            color #333
            margin-bottom 14px
        .p-link-input
            input
                border 1px solid #cccccc
                border-radius 2px
                width 100%
                height 30px
            .link-input-wrap
                display flex
                align-items center
                .add-icon
                    display inline-block
                    margin-left 8px
                    height 24px
                    width 24px
                    background url("~statics/images/common/u51.png") center no-repeat
                    cursor pointer
        .p-catalog-left
            border-right 1px solid #e5e5e5
            box-sizing border-box
            overflow hidden
            height 226px
            .p-radio-wrap
                height 100%
                padding 32px 16px                  
            .p-g-radio
                display flex 
                &:not(:first-child)
                    margin-top 0.6rem
                .q-option-inner.active
                    color theme-color
                .q-option-inner + .q-option-label
                    font-family font-r
                    color #333333
                    font-size 12px
                    white-space nowrap

        .p-catalog-right
            height 226px
            padding 0 16px
            display flex
            flex 1
            align-items center
            .p-right-content
                width 100%
                .p-no-link
                    height 92px
                    background url("~statics/images/common/none-link.png") no-repeat center center
                .p-no-link-text
                    font-family font-r
                    font-size 14px
                    color #666
            .q-if
                border 1px solid #ccc
                margin-top 0
                margin-bottom 0
                padding-bottom 0
                &:before
                    height 0
                .q-input-target
                    padding-left 10px
                &:after
                    display none
        .btn-wrap
            padding 16px
            text-align right
            .btn-change
                position relative
                display inline-block
                line-height 1
                cursor pointer
                white-space nowrap
                border 1px solid #dcdfe6
                color #606266
                text-align center
                outline 0
                margin 0
                transition .1s
                font-weight 500
                padding 12px 20px
                font-size 14px
                border-radius 4px
                background linear-gradient(180deg,#fff 5%,#f1f3f7)
            .btn-size
                color #fff
                border-color #409eff
                background linear-gradient(180deg,#2ca0f7 5%,#1585d8)
                position relative
                display inline-block
                line-height 1
                cursor pointer
                white-space nowrap
                border 1px solid #dcdfe6
                text-align center
                outline 0
                margin 0
                transition .1s
                font-weight 500
                padding 12px 20px
                font-size 14px
                border-radius 4px
                margin-left 10px
.w-common-select
    position relative
    cursor pointer
    .select-default
        height 30px
        line-height 30px
        font-size 14px
        padding-right 8px
        padding-left 10px
        box-sizing border-box
        border 1px solid #ccc
        cursor pointer
        &:after
            content ""
            display inline-block
            height 30px
            width 20px
            background url("~statics/images/common/downsingle.png") no-repeat center center
            position absolute
            right 4px
            top 0
    .option-box
        cursor pointer
        width 100%
        margin 0
        padding 0
        position absolute 
        left 0
        top 32px
        background-color #fff
        border 1px solid #ccc
        max-height 210px
        overflow-y auto
        z-index 1000
        .option-item
            height 42px
            line-height 42px
            color #333
            font-family font-r
            font-size 16px
            padding-left 20px
            &:not(:last-child)
                border-bottom 1px solid #f1f4f8
            &:hover
                background-color #f1f4f8
                color #00ABFF
.pop-framework
    width 890px
    border 1px solid #ccc
    border-radius 4px
    position fixed
    top 50%
    left 50%
    transform translate(-50%, -50%)
    z-index 1000
    box-shadow 0px 0px 30px #ccc
    background-color #fff
    .pop-header
        height 44px
        line-height 44px
        border-top-left-radius 4px
        border-top-right-radius 4px
        padding-left 20px
        color #fff
        font-size 16px
        background-color #038cd6
        cursor move
    .pop-content
        padding 0 20px
        min-height 270px
        overflow auto
        .table-search
            padding 16px 0
            text-align right
            display flex
            justify-content flex-end
            .search-wrap
                position relative
                border 1px solid #ccc
                border-radius 4px
                .search-input
                    height 28px
                    line-height 28px
                    width 254px
                    outline none
                    padding-right 36px
                    color #333
                    font-family  Roboto-r
                    font-size 14px
                    border-radius 4px
                    border 0
                    background-color #fff
                .search-icon
                    cursor pointer
                    display inline-block
                    position absolute
                    right 1px
                    top 0
                    width 36px
                    height 100%
                    background url("~statics/images/common/search.png") center center no-repeat
                    background-color #fff
                    &:hover
                        opacity 0.9
