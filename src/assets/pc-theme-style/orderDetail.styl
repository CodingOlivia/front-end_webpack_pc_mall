      .info-item2
        margin-left 16px
.delivery-wrap
  
  .delivery-title
     font-size 24px
     font-weight bold
     margin 20px 0
  .delivery-inner-wrapper
     border 1px solid $border-color-1
     background #fff
  .main
     margin 0 auto
     width 82%
  .delivery-tabs
     
     display flex
     position relative
     span
       display block
       padding 8px 40px
       text-align center
       border-radius 25px
       background $background-color-5
       position relative
       cursor pointer
       border-right 4px solid #fff
       border-left 4px solid #fff
       margin-left  -25px
       color $all-color-3
       z-index 2
       &:first-child
          border-left none
          //padding-left 16px
          margin-left 0
          border-radius 25px 25px 25px 0
       &:last-child
          border-radius 25px 25px  0 25px
       &.active
          background $positive
          color $all-color-1
          z-index 1000!important
  .delivery-con
     overflow-y auto
     max-height 340px
     border-bottom 1px solid $border-color-1
     .delivery-con-wrapper
          display flex
          align-items stretch
          .delivery-wrapper-left
            width 486px
            .delivery-pro
              margin 16px 0 16px 32px
              display flex
              padding-bottom 16px
              border-bottom 1px solid $border-color-1

              .delivery-pro-img
                width 80px
                height 80px
                margin-right 10px
                text-align center
                img
                    max-width 100%
                    max-height 100%
                    display inline-block
              .delivery-pro-info
                flex 1
                overflow hidden
                align-items stretch
                .pro-title
                    white-space nowrap
                    overflow hidden
                    text-overflow ellipsis
                    width 100%
                    margin-bottom 8px
                .pro-prop
                    font-size 12px
                    color $font-color-5 
                    margin-bottom 5px
                    &.pro-prop-share
                       color $background-color-4
              .pro-count
                  min-width 120px
                  padding 0 30px 0 16px
                  span
                    display block
                    position relative
                    text-align right
                    color $font-color-5 
                    top 50%
                    transform: translateY(-50%)
              &:last-child
                margin-bottom 0
                border none
          .delivery-wrapper-right
              border-left 1px solid $border-color-1
              padding 20px
              position relative
              .orderStatus
                  position absolute
                  top 50%
                  left 50%
                  transform: translate(-50%,-50%)
                  font-size 20px
              .info-step
                .info-step-item
                  display flex
                  .info-step-icon
                    position relative
                    .info-step-dot
                      width 12px
                      height 12px
                      position absolute
                      background #ccc
                      top 50%
                      border 2px solid #fff
                      transform translateY(-50%)
                      //background url("~statics/images/orderDetail/dotted.png") no-repeat
                      //background-position 0 -14px
                      border-radius 50%
                      margin-left 1px
                    .l-t
                      display inline-block
                      height 100%
                      width 2px
                      background #ccc
                      position absolute
                      left 6px
                      top 0
                  .info-step-text
                    padding 15px
                    margin-left 20px
                    .tit
                      font-size 12px
                      color $font-color-5
                      margin-bottom 4px
                      display inline-block
                      margin-right 25px
                    .desc
                      display inline-block
                      span
                        font-size 14px
                        //color #333
                        margin-right 20px
                  &.active
                    .info-step-icon
                      .info-step-dot
                        //background-position 0 0
                        background $positive
                        width 16px
                        height 16px
                        border none
                        margin-left -1px
                      .l-t
                        background $positive
                    .info-step-text
                      .tit
                        color $positive
                      .desc
                        span
                          color $positive
  .delivery-footer
      padding 16px 32px
      background #fafafa
      .delivery-text
        font-size 14px
        .delivery-item
          margin-bottom 12px
          color $font-color-5
          b 
            color $all-color-1
        .delivery-desc
            margin-right 25px
            color $font-color-5
            b  
               color $all-color-1
.orderStatue-wrap
    .main
      width 82%
      margin 0 auto
    .orderStatue-inner
        border 1px solid $border-color-1
        .orderStatue-left
          padding 50px
          width 486px
          border-right 1px solid $border-color-1
          .order-num
             color $font-color-3
             span
                color $all-color-1
          .order-name
             font-size 24px
             color $positive
             margin-bottom 8px
          .order-action
             padding-top  40px 
             .btn
                padding 6px 16px
                border 1px solid $border-color-1
                border-radius 32px
                margin-right 12px
                text-align center
                cursor pointer
                display inline-block
                width calc( 100% - 8px)
                white-space nowrap
                overflow hidden
                text-overflow ellipsis
                &.primary
                  color $all-color-2
                  background  $positive
                  border-color $positive
              .text-btn
                cursor pointer
                color $font-color-4
        .orderStatue-right
          padding 16px
          .order-process
              position relative
              top 50%
              transform translateY(-50%)
              .order-process-step
                  text-align center
                  padding 0 20px
                  position relative
                  .order-step-img
                    display inline-block
                    width 40px
                    height 40px
                    margin-bottom 16px
                  .order-process-title
                    margin-bottom 8px
                    color $font-color-3
                  .order-process-date
                    color $font-color-3
                  &:before
                    content ''
                    position absolute
                    display block
                    width calc( 100% - 100px)
                    border-bottom 1px dashed $font-color-5 
                    left calc( 50px - 50%)
                    TOP 20px
                  &:first-child      
                    &:before
                        opacity 0
                  .order-step01
                    background  url('~statics/images/orderDetail/h-step1.png')  no-repeat
                    background-size 100%
                  .order-step02
                    background  url('~statics/images/orderDetail/h-step2.png')  no-repeat
                    background-size 100%
                  .order-step03
                    background  url('~statics/images/orderDetail/h-step3.png')  no-repeat
                    background-size 100%
                  .order-step04
                    background  url('~statics/images/orderDetail/h-step4.png')  no-repeat
                    background-size 100%
                  .order-step05
                    background  url('~statics/images/orderDetail/h-step5.png')  no-repeat
                    background-size 100%
                  .order-step06
                    background  url('~statics/images/orderDetail/h-step6.png')  no-repeat
                    background-size 100%
                  &.active
                    .order-step01
                      background  url('~statics/images/orderDetail/step1.png')  no-repeat
                      background-size 100%
                    .order-step02
                      background  url('~statics/images/orderDetail/step2.png')  no-repeat
                      background-size 100%
                    .order-step03
                      background  url('~statics/images/orderDetail/step3.png')  no-repeat
                      background-size 100%
                    .order-step04
                      background  url('~statics/images/orderDetail/step4.png')  no-repeat
                      background-size 100%
                    .order-step05
                      background  url('~statics/images/orderDetail/step5.png')  no-repeat
                      background-size 100%
                    .order-step06
                      background  url('~statics/images/orderDetail/step6.png')  no-repeat
                      background-size 100%
                    &:before
                       border-color $positive
                    .order-process-title
                       color $positive
                 
    .orderStatue-title
        font-size 24px
        font-weight bold
        margin 20px 0
        .orderStatue-inner
            .orderStatue-left
                padding 16px  
   