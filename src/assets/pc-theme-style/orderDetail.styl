.remark-wrap
    margin  16px 0
    .main
      width: 82%
      margin 0 auto
      .remark-con
          padding 16px
          border 1px solid $border-color-1
          min-height 100px
.recomment-wrap
    margin-bottom 16px
    .main
      margin 0 auto
    .search-hot
      display flex
      flex-direction column
      .hot-top
        padding 20px 0
        display flex
        justify-content space-between
        font-size 12px
        .hot-left
          display flex
          align-items center
          span
             font-size calc(24/1920*100vw)
             font-weight bold
          .q-if
            padding 0
            margin 0
        .hot-right
          display flex
          align-items center
          .hot-btn-left
            cursor pointer
            width calc(36/1920*100vw)
            height calc(36/1920*100vw)
            background $all-color-1
            border-radius 50px
            color $all-color-2
            display flex
            align-items center
            justify-content center
            margin-right calc(20/1920*100vw)
            font-size calc(14/1920*100vw)
            min-width 24px
            min-height 24px
            opacity 1
            &.swiper-button-disabled
              opacity .5
          .hot-btn-right
            cursor pointer
            width calc(36/1920*100vw)
            height calc(36/1920*100vw)
            background $all-color-1
            border-radius 50px
            color $all-color-2
            display flex
            align-items center
            justify-content center
            font-size calc(14/1920*100vw)
            min-width 24px
            min-height 24px
            opacity 1
            &.swiper-button-disabled
              opacity .5
          span
            padding 0 12px
            display none
      .hot-main
        width 100%
        display flex
        justify-content flex-start
        align-items center
        margin 0 0 6% 0
        .hot-box:hover
          border 1px solid #000
        .hot-box
          display inline-block
          float left
          height 100%
          border 1px solid #c5c5c5
          margin-right calc(32/1920*100vw)
          .offering-box
            display flex
            flex-direction column
            align-items center
            justify-content flex-start
            width 100%
            .offering-img
              width calc(270/1920*100vw)
              display flex
              justify-content center
              align-items flex-start
              img
                width 100%
            .offering-text
              margin: 4% 2% 10% 2%;
              width 100%
              display flex
              align-items center
              flex-direction column
              .offering-name
                width 100%
                font-size calc(18/1920*100vw)
                color $all-color-1
                text-align center
                font-weight bold
              .offering-price
                width 100%
                font-size calc(16/1920*100vw)
                color $all-color-1
                margin 0
                display flex
                align-items center
                justify-content center
              p
                letter-spacing 0
                margin 0
                line-height 2
                padding 0
      .search-hot-main
        // width calc(1483/1920*100vw)
        width 100%
        //height calc(380/1920*100vw)
        height 380px
        position relative
        overflow hidden
      .search-hot-box
        display inline-block
        position absolute
        display flex
        transition all .3s
      .search-hot-img
        border 1px solid #c5c5c5
        display inline-block
        //height calc(380/1920*100vw)
        height 380px
        box-sizing border-box
        display flex
        justify-content flex-start
        align-items center
        flex-direction column
        overflow hidden
        .search-hot-imgname
          font-weight bold
          //font-size calc(18/1920*100vw)
          font-size 18px
          color $all-color-1
          line-height 2
          margin-top calc(20/1920*100vw)
          padding 0 16px
          white-space nowrap
          overflow hidden
          width 100%
          text-align center
          text-overflow ellipsis
        .search-hot-data
          font-weight 400
          //font-size calc(16/1920*100vw)
          font-size 16px
          display flex
          flex-direction row
          padding 0 16px
          color $all-color-1
          .search-hot-imgtype
            padding-left 10px
            color $font-color-3
      .search-hot-img:hover
        border 2px solid $all-color-1
.info-wrap
   .main
      margin 0 auto
      width 82%
      .info-title
        font-size 24px
        margin 20px 0
        font-weight bold
      .pl16
        padding-left 16px
      .info-item
        border 1px solid $border-color-1
        padding 16px 30px
        &>div
          margin 16px 0
          .info-prop
             display inline-block
             min-width 150px
             color $font-color-5
      .info-item1
